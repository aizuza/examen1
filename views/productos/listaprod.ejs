<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>
        <%= titulo %>
    </h1>
    <a href="/nuevo">Nuevo producto</a>
    <ul>
        <% productos.forEach(producto => { %>
            <li>
                <p>
                    <%= producto.nombre %> ($
                        <%= producto.precio %> )</p>
                <p>id del producto:
                    <%= producto.id %>
                </p>
            </li>
            <a href="/editar/<%= producto.id %>">Editar</a>
            <a href="/ver/<%= producto.id %>">Ver Comentarios</a>
            <% }) %>
    </ul>
</body>

</html>